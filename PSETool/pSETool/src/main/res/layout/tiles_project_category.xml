<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/LinearLayout1"
    style="@android:style/Holo.ButtonBar"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
        style="@android:style/DeviceDefault.ButtonBar"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:baselineAligned="false" >

        <FrameLayout
            style="@android:style/ButtonBar"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1" >

            <ImageButton
                android:id="@+id/project_category_roof_imgbtn"
                style="@android:style/ButtonBar"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:background="@android:drawable/screen_background_light"
                android:contentDescription="roofimageicon"
                android:paddingBottom="20dp"
                android:scaleType="fitCenter"
                android:src="@drawable/ic_deserttanshingle" />

            <TextView
                android:id="@+id/TextView3"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:gravity="center_horizontal"
                android:text="Roof"
                android:textAppearance="?android:attr/textAppearanceButton" />
        </FrameLayout>

        <FrameLayout
            style="@android:style/ButtonBar"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1" >

            <ImageButton
                android:id="@+id/project_category_door_imgbtn"
                style="@android:style/ButtonBar"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:adjustViewBounds="true"
                android:background="@android:drawable/screen_background_light"
                android:contentDescription="doorcategory button"
                android:paddingBottom="20dp"
                android:scaleType="fitCenter"
                android:src="@drawable/ic_frenchdoor" />

            <TextView
                android:id="@+id/TextView4"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:gravity="center_horizontal"
                android:text="Door"
                android:textAppearance="?android:attr/textAppearanceButton" />
        </FrameLayout>
    </LinearLayout>

    <LinearLayout
        style="@android:style/DeviceDefault.ButtonBar"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:baselineAligned="false" >

        <FrameLayout
            style="@android:style/ButtonBar"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1" >

            <ImageButton
                android:id="@+id/project_category_gutter_imgbtn"
                style="@android:style/ButtonBar"
                android:layout_width="fill_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@android:drawable/screen_background_light"
                android:contentDescription="gutter category button"
                android:paddingBottom="20dp"
                android:scaleType="fitCenter"
                android:src="@drawable/ic_launcher_gutter" />

            <TextView
                android:id="@+id/TextView2"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:gravity="center_horizontal"
                android:text="Gutter"
                android:textAppearance="?android:attr/textAppearanceButton" />
        </FrameLayout>

        <FrameLayout
            style="@android:style/ButtonBar"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1" >

            <ImageButton
                android:id="@+id/project_category_window_imgbtn"
                style="@android:style/ButtonBar"
                android:layout_width="fill_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@android:drawable/screen_background_light"
                android:contentDescription="window category button"
                android:paddingBottom="20dp"
                android:scaleType="fitCenter"
                android:src="@drawable/ic_launcher_doublewindow" />

            <TextView
                android:id="@+id/TextView1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:gravity="center_horizontal"
                android:text="Window"
                android:textAppearance="?android:attr/textAppearanceButton" />
        </FrameLayout>
    </LinearLayout>

    <LinearLayout
        style="@android:style/DeviceDefault.ButtonBar"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:baselineAligned="false" >

        <FrameLayout
            style="@android:style/ButtonBar"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1" >

            <ImageButton
                android:id="@+id/project_category_siding_imgbtn"
                style="@style/styleButtonBar"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:background="@android:drawable/screen_background_light"
                android:contentDescription="siding category button"
                android:paddingBottom="20dp"
                android:scaleType="fitCenter"
                android:src="@drawable/ic_vinyl_siding_gproundshingle" />

            <TextView
                android:id="@+id/TextView6"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:gravity="center_horizontal"
                android:text="Siding"
                android:textAppearance="?android:attr/textAppearanceButton" />
        </FrameLayout>

        <FrameLayout
            style="@android:style/ButtonBar"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1" >

            <ImageButton
                android:id="@+id/project_category_fencing_imgbtn"
                style="@style/styleButtonBar"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:background="@android:drawable/screen_background_light"
                android:contentDescription="fence category button"
                android:paddingBottom="20dp"
                android:scaleType="fitCenter"
                android:src="@drawable/ic_vinyl_fence_beige_latticetop" />

            <TextView
                android:id="@+id/TextView5"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:gravity="center_horizontal"
                android:text="Fence"
                android:textAppearance="?android:attr/textAppearanceButton" />
        </FrameLayout>
    </LinearLayout>

</LinearLayout>